<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Paleta de colores principal-->
    <SolidColorBrush x:Key="primaryColor1" Color="#5F5FFF"/><!--Azul-->
    <SolidColorBrush x:Key="primaryColor2" Color="#E8EEF2"/><!--Blanco/Azul-->
    <SolidColorBrush x:Key="primaryColor3" Color="#2F2F36"/><!--Negro/Azul-->
    <SolidColorBrush x:Key="primaryColor4" Color="DarkBlue"/>

    <SolidColorBrush x:Key="BackgroudColor" Color="#FFEDEDED"/><!--Blanco-->


    <SolidColorBrush x:Key="Color1" Color="#E5E5E5"/>


    <!--Colores de texto-->
    <SolidColorBrush x:Key="titleColorA" Color="#5F5FFF"/>
    <SolidColorBrush x:Key="titleColorB" Color="DarkBlue"/>
    
    <SolidColorBrush x:Key="titleColor1" Color="Black"/>
    <SolidColorBrush x:Key="titleColor2" Color="#565360"/>
    <SolidColorBrush x:Key="titleColor3" Color="#908E9B"/>
    <SolidColorBrush x:Key="titleColor4" Color="#E1DFE9"/>
    

    
    <!--Button Colors-->
    <SolidColorBrush x:Key="buttonColor1" Color="#6666FF"/>
    <SolidColorBrush x:Key="buttonColor2" Color="#274288"/>
    
    <!--TopBar-->
    <SolidColorBrush x:Key="buttonMin" Color="LightBlue"/>
    <SolidColorBrush x:Key="buttonMax" Color="LightGreen"/>
    <SolidColorBrush x:Key="buttonExit" Color="LightCoral"/>
    
    <!--Textbox-->
    <SolidColorBrush x:Key="BoxColor1" Color="#6666FF"/>

    <!--Colores borde de ventana LOGIN-->
    <Color x:Key="winBorderColor1">#FF304B99</Color>
    <Color x:Key="winBorderColor2">#7D14ED</Color>
    <Color x:Key="winBorderColor3">#EE1311</Color>


</ResourceDictionary>
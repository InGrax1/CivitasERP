    <!--Colores borde de ventana-->
    <Color x:Key="winBorderColor1">#FF304B99</Color>
    <Color x:Key="winBorderColor2">#7D14ED</Color>
    <Color x:Key="winBorderColor3">#EE1311</Color>
    
